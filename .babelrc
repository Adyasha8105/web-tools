{
  "presets": ["es2015", "stage-2", "react"],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-hot-loader/babel"],
        ["transform-react-jsx-source"],
        ["react-intl", {
          "messagesDir": "./build/messages/"
        }]
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
